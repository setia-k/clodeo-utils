{
  "env": {
    "es2021": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "objectLiteralDuplicateProperties": false
    }
  },
  "ignorePatterns": [
    "node_modules/",
    "/lib"
  ],
  "rules": {
    "no-multi-spaces": "error",
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
